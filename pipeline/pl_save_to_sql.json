{
	"name": "pl_save_to_sql",
	"properties": {
		"activities": [
			{
				"name": "Copy data fom api to sql",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['driverId']"
								},
								"sink": {
									"name": "driverId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['permanentNumber']"
								},
								"sink": {
									"name": "permanentNumber",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['code']"
								},
								"sink": {
									"name": "code",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['url']"
								},
								"sink": {
									"name": "url"
								}
							},
							{
								"source": {
									"path": "['givenName']"
								},
								"sink": {
									"name": "givenName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['familyName']"
								},
								"sink": {
									"name": "familyName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['dateOfBirth']"
								},
								"sink": {
									"name": "dateOfBirth",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['nationality']"
								},
								"sink": {
									"name": "nationality",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['MRData']['DriverTable']['season']"
								},
								"sink": {
									"name": "season",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['MRData']['DriverTable']['round']"
								},
								"sink": {
									"name": "round",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['MRData']['xmlns']"
								},
								"sink": {
									"name": "xmlns",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['MRData']['series']"
								},
								"sink": {
									"name": "series",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['MRData']['total']"
								},
								"sink": {
									"name": "total",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['MRData']['DriverTable']['Drivers']",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "http_json",
						"type": "DatasetReference",
						"parameters": {
							"relative_url": "drivers.json"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "target_sql",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}